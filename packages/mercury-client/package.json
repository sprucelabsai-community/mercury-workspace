{
  "name": "@sprucelabs/mercury-client",
  "publishConfig": {
    "access": "public"
  },
  "version": "31.0.112",
  "files": [
    "build"
  ],
  "skill": {
    "namespace": "mercuryClient",
    "commandOverrides": {
      "sync.schemas": "--shouldFetchCoreSchemas false"
    },
    "upgradeIgnoreList": [
      "@sprucelabs/spruce-test-fixtures",
      "@sprucelabs/mercury-event-emitter",
      "@sprucelabs/spruce-skill-utils"
    ]
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "module": "./build/esm/index.js",
  "sideEffects": false,
  "description": "The simple way to interact with the Spruce Experience Platform",
  "keywords": [
    "node",
    "components",
    "sprucebot",
    "sprucelabs"
  ],
  "homepage": "https://github.com/sprucelabsai/mercury-workspace",
  "bugs": {
    "url": "https://github.com/sprucelabsai/mercury-workspace/issues"
  },
  "scripts": {
    "build.ci": "yarn build.tsc && yarn build.resolve-paths && yarn lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.dev": "yarn build.tsc --sourceMap ; yarn resolve-paths.lint",
    "build.dist": "tsc --project tsconfig.dist.json && yarn build.resolve-paths && mv build esm && yarn build.esm-postbuild && yarn build.tsc && yarn build.resolve-paths && mv esm build/ && yarn clean.dist",
    "build.esm-postbuild": "esm-postbuild --target esm --patterns '**/*.js'",
    "build.tsc": "yarn build.copy-files && tsc",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "clean": "yarn clean.build",
    "clean.all": "yarn clean.dependencies && yarn clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "clean.dist": "rm -rf build/__tests__ build/esm/__tests__",
    "fix.lint": "eslint --fix --cache '**/*.ts'",
    "lint": "eslint --cache '**/*.ts'",
    "lint.tsc": "tsc -p . --noEmit",
    "post.watch.build": "yarn build.copy-files && yarn build.resolve-paths",
    "rebuild": "cd .. && yarn rebuild",
    "release": "semantic-release",
    "resolve-paths.lint": "yarn build.resolve-paths ; yarn lint",
    "test": "jest",
    "update.dependencies": "cd ../ && yarn rebuild.dependencies",
    "upgrade.packages": "yarn-upgrade-all && rm -f yarn.lock ; yarn ; yarn fix.lint ; true",
    "upgrade.packages.all": "yarn install && yarn upgrade.packages",
    "upgrade.packages.test": "yarn upgrade.packages.all && yarn lint && yarn build.dev && yarn test",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn post.watch.build'",
    "watch.lint": "concurrently 'yarn lint' \"chokidar 'src/**/*' -c 'yarn lint.tsc'\"",
    "watch.rebuild": "yarn clean.all && yarn && yarn watch.build.dev",
    "watch.tsc": "tsc -w"
  },
  "dependencies": {
    "@sprucelabs/error": "^5.0.490",
    "@sprucelabs/schema": "^28.4.2",
    "@sprucelabs/spruce-core-schemas": "^34.2.28",
    "@sprucelabs/spruce-event-utils": "^27.0.88",
    "dotenv": "^16.0.1",
    "just-clone": "^6.0.1",
    "socket.io-client": "^4.5.1"
  },
  "devDependencies": {
    "@sprucelabs/esm-postbuild": "^1.0.538",
    "@sprucelabs/jest-json-reporter": "^6.0.434",
    "@sprucelabs/jest-sheets-reporter": "^1.3.69",
    "@sprucelabs/mercury-core-events": "^12.0.113",
    "@sprucelabs/mercury-event-emitter": "^31.0.112",
    "@sprucelabs/mercury-types": "^35.0.70",
    "@sprucelabs/resolve-path-aliases": "^1.1.77",
    "@sprucelabs/semantic-release": "^4.0.8",
    "@sprucelabs/test": "^7.7.340",
    "@sprucelabs/test-utils": "^3.1.43",
    "@types/node": "^18.6.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.21.0",
    "eslint-config-spruce": "^10.11.3",
    "jest": "^28.1.3",
    "jest-circus": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "maxWorkers": 2,
    "testTimeout": 60000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/__tests__/testDirsAndFiles/",
      "<rootDir>/build/esm/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    },
    "reporters": [
      "default",
      [
        "@sprucelabs/jest-sheets-reporter",
        {
          "sheetId": "1DoWM7mYgGDa-PJXptLomrZmbIBZiuxS1YiAsRXU2Gm0",
          "worksheetId": 843037832,
          "testMap": {
            "cantEmitEventWithWithUnexpectedPayload": "F8",
            "factoryReturnsSocketIoClient": "F12",
            "canRunHealthCheck": "F13",
            "skillsCanListenToEachOther": "F22",
            "emitterGetsCalledBackForEachListener": "F23",
            "canRegisterEventsSimultaneously": "I102",
            "nonSpruceErrorsArePassedBack": "I111",
            "checksPermissionsWhenEmitting": "I138",
            "invokesReconnectAfterDelayWhenConnectionLost": "I326",
            "retriesEventIfItFailsAfterTimeout": "I327",
            "canReAuthIfTimesOutDuringAuth": "I334"
          }
        }
      ]
    ]
  }
}
